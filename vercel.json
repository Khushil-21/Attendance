{
	"version": 2,
	"builds": [
		{
			"src": "src/Backend/Authentication.js",
			"use": "@vercel/node"
		},
		{ "src": "src/Backend/**/*", "use": "@vercel/node" },
		{ "src": "src/Database/**/*", "use": "@vercel/node" }
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/Backend/Authentication.js"
		}
	]
}
